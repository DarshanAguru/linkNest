{
  "expo": {
    "name": "linknest",
    "slug": "linknest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_tiny.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#25292e"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon_tiny.png"
    },
    "androidStatusBar": {
      "backgroundColor": "#25292e",
      "translucent": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon_tiny.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.darshanaguru.linknest",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon_tiny.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon_tiny.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#25292e"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/WinkyRough-VariableFont_wght.ttf"
          ]
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          },
          "ios": {
            "customBuildFlags": [
              "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    },
    "owner": "darshanaguru"
  }
}